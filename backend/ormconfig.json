{
  "type": "mysql",
  "url": "${process.env.DATABASE_URL}",
  "entities": ["dist/**/*.entity{.js,.ts}"],
  "synchronize": true,
  "logging": false,
  "dropSchema": false,
  " migrationsTableName": "migrations",
  "migrations": ["src/migrations/*.ts"],
  "cli": {
    "migrationsDir": "src/migrations"
  }
}
